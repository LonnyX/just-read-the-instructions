{
  "name": "generalsystemsvehicle/just-read-the-instructions",
  "description": "Wordpress Gutenberg as a Laravel Nova field type",
  "type": "library",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "homepage": "https://github.com/generalsystemsvehicle/just-read-the-instructions",
  "readme": "README.md",
  "authors": [
    {
      "name": "Lee Hilton",
      "email": "lee.hilton@leezilla.net",
      "homepage": "https://leezilla.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "unisharp/laravel-filemanager": "^2.0",
    "van-ons/laraberg": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "GeneralSystemsVehicle\\JustReadTheInstructions\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "GeneralSystemsVehicle\\JustReadTheInstructions\\FieldServiceProvider"
      ],
      "aliases": {
        "JustReadTheInstructions": "GeneralSystemsVehicle\\JustReadTheInstructions\\Facade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
