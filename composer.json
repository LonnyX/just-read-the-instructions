{
    "name": "general-systems-vehicle/just-read-the-instructions",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "unisharp/laravel-filemanager": "^1.9",
        "van-ons/laraberg": "~1.0.3"
    },
    "autoload": {
        "psr-4": {
            "GeneralSystemsVehicle\\JustReadTheInstructions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneralSystemsVehicle\\JustReadTheInstructions\\FieldServiceProvider"
            ],
            "aliases": {
                "JustReadTheInstructions": "GeneralSystemsVehicle\\JustReadTheInstructions\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
